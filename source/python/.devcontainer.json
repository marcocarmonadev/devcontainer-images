{
    "build": {
        "dockerfile": "Dockerfile"
    },
    "features": {
        "ghcr.io/devcontainers/features/common-utils": {
            "installZsh": false,
            "installOhMyZsh": false,
            "installOhMyZshConfig": false,
            "username": "vscode",
            "userUid": 1000,
            "userGid": 1000
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "VisualStudioExptTeam.vscodeintellicode"
            ]
        }
    },
    "remoteUser": "vscode"
}